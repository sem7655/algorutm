def insertion_sort(arr):# объвляем функцию
    for i in range(1, len(arr)):
        key = arr[i]
        j = i - 1
        while j >= 0 and arr[j] > key:
            arr[j+1] = arr[j]
            j -= 1
        arr[j+1] = key
    return arr
arr = [5, 4, 3, 2, 1]
sorted_arr = insertion_sort(arr)
print(sorted_arr)
